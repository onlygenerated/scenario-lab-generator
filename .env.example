# ScenarioLabGenerator environment configuration
# Copy to .env and fill in your values

# Anthropic API key (required for AI generation, not needed for demo mode)
ANTHROPIC_API_KEY=sk-ant-your-key-here

# AI model (default: claude-sonnet-4-5-20250514)
# ANTHROPIC_MODEL=claude-sonnet-4-5-20250514

# API server
# API_HOST=0.0.0.0
# API_PORT=8000

# CORS (comma-separated origins)
# CORS_ORIGINS=["http://localhost:5173"]

# Docker lab port range
# LAB_PORT_RANGE_START=8888
# LAB_PORT_RANGE_END=8988

# Lab workspace directory
# LAB_BASE_DIR=./lab_workspaces

# Rate limiting
# GENERATE_RATE_LIMIT_PER_MINUTE=10

# Demo mode (set to true to skip AI and use sample blueprint)
DEMO_MODE=true
